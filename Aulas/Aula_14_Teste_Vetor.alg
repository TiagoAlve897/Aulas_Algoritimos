algoritmo "Teste_Vetor"

// Autor : Tiago
// Data : 12/11/2017

var
v: vetor [1..6] de inteiro
c: Inteiro

inicio
      Para c <- 1 ate 6 faca
           Escreva("Digite o ", c,"o. valor " )
           Leia(v[c])
      FimPara
      Para c <- 1 ate 6 faca
           Escreva("{", v[c], "}")
      Fimpara
fimalgoritmo
