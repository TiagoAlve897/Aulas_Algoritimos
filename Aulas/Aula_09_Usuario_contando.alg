algoritmo "09_Usuario_contando"

var cont1,cont2,contador:inteiro

inicio

contador <- 0
Escreval("Voçe quer contar ate que numero")
Leia (cont1)
Escreval("quer saltar ate quanto")
Leia(cont2)
Enquanto (contador <= cont1) faca
Escreval(contador)
contador <- contador + cont2
FimEnquanto

fimalgoritmo
